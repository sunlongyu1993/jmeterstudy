			dbcustomerNames = vars.getObject(&quot;crmDBResult&quot;).get(i).get(&quot;customer_name&quot;);//从数据库返回结果中读取数据
			log.info(&quot;数据库dbcustomerIds:&quot;+dbcustomerIds+&quot;,数据库dbcustomerNames:&quot;+dbcustomerNames);
			if(customerNames.equals(dbcustomerNames)&amp;&amp;customerIds.equals(dbcustomerIds)){
				Failure =false;
				log.info(&quot;断言成功&quot;);
				}
			else{
				Failure =true;
				FailureMessage= &quot;数据库dbcustomerId:&quot;+dbcustomerId
